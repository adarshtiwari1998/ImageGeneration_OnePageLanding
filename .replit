modules = ["web", "nodejs-20"]
[agent]
expertMode = true

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Static Web Server"

[[workflows.workflow]]
name = "Static Web Server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npx http-server . -p 5000 -a 0.0.0.0 --cors"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 33463
externalPort = 3000

[[ports]]
localPort = 42577
externalPort = 3001

[nix]
channel = "stable-25_05"

[deployment]
deploymentTarget = "autoscale"
run = ["npx", "http-server", ".", "-p", "5000", "-a", "0.0.0.0", "--cors"]
