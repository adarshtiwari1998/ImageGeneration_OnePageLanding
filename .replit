externalPort = 80

[[ports]]
localPort = 35737
externalPort = 3000

[[ports]]
localPort = 42577
externalPort = 3001

[nix]
channel = "stable-25_05"

[deployment]
deploymentTarget = "autoscale"
run = ["node", "server.js"]
